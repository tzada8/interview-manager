<p id="notice"><%= notice %></p>

<p>
  <strong>Position:</strong>
  <%= @interview.position %>
</p>

<p>
  <strong>Company:</strong>
  <%= @interview.company %>
</p>

<p>
  <strong>Date:</strong>
  <%= @interview.date %>
</p>

<p>
  <strong>Duration:</strong>
  <%= @interview.duration %>
</p>

<p>
  <strong>Interviewer:</strong>
  <%= @interview.interviewer %>
</p>

<p>
  <strong>Industry:</strong>
  <%= @interview.industry %>
</p>

<p>
  <strong>Questions:</strong>
  <% @interview.questions.each do |question| %>
    <ul>
      <li>
        <%= question.prompt %>
        <%= question.answer %>
      </li>
    </ul>
  <% end %>
</p>

<%= link_to 'Edit', edit_interview_path(@interview) %> |
<%= link_to 'Add Question', interview_questions_path(@interview) %> |
<%= link_to 'Back', interviews_path %>
